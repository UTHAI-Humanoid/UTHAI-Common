                <mass value="0.10500000"/>
                <inertia ixx="0.00123572" ixy="0.00000500" ixz="0.00000812" iyy="0.00031773" iyz="0.00000828" izz="0.00094763" />
            </inertial>
        </link>
    </xacro:macro>
