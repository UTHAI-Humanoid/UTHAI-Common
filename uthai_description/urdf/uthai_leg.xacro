                </geometry>
                <material name="grey"/>
            </visual>
            <inertial>
                <origin xyz="0.21946422 -0.00013856 0.00003754"/>
                <mass value="0.29655036"/>
                <inertia ixx="0.00010413" ixy="-0.00000267" ixz="0.00000165" iyy="0.00255502" iyz="0.00000000" izz="0.00251726" />
            </inertial>
        </link>
        <!-- left knee pitch -->
        <joint name="l_knee_pitch_joint" type="revolute">
            <axis xyz="0 0 1" />
