<?xml version="1.0"?>
<robot 
    xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:macro name="insert_transmission" params="name">
        <transmission name="${name}_tran">
            <type>transmission_interface/SimpleTransmission</type>
            <joint name="${name}">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            </joint>
            <actuator name="${name}_motor">
                <hardwareInterface>EffortJointInterface</hardwareInterface>
                <mechanicalReduction>1</mechanicalReduction>
            </actuator>
        </transmission>
    </xacro:macro>
    <xacro:macro name="uthai_left_leg" params="parent">
        <!-- left hip yaw -->
        <joint name="l_hip_yaw_joint" type="revolute">
            <axis xyz="0 0 1" />
            <origin xyz="0.008 0.09 -0.06" rpy="0 0 ${pi/2}"/>
            <parent link="${parent}"/>
            <child link="l_hip_yaw_link"/>
            <limit effort="10" lower="${-pi*0.9}" upper="${pi*0.9}" velocity="5.6548668"/>
            <dynamics damping="0.1" friction="0.0"/>
        </joint>
        <xacro:insert_transmission name="l_hip_yaw_joint"/>
        <link name="l_hip_yaw_link">
            <collision>
                <geometry>
                    <mesh filename="package://uthai_description/meshes/l_hip_yaw.STL"/>
                </geometry>
            </collision>
            <visual>
                <geometry>
                    <mesh filename="package://uthai_description/meshes/l_hip_yaw.STL"/>
                </geometry>
                <material name="grey"/>
            </visual>
            <inertial>
                <origin xyz="0.00000000 0.02500000 -0.00735017"/>
                <mass value="0.090718"/>
                <inertia ixx="0.00014114" ixy="0.00000000" ixz="0.00000000" iyy="0.00002016" iyz="0.00000000" izz="0.00014272" />
            </inertial>
        </link>
        <!-- left hip roll -->
        <joint name="l_hip_roll_joint" type="revolute">
            <axis xyz="0 0 1" />
            <origin xyz="0 0 -0.036" rpy="${pi/2} 0 0"/>
            <parent link="l_hip_yaw_link"/>
            <child link="l_hip_roll_link"/>
            <limit effort="10" lower="${-pi*0.9}" upper="${pi*0.9}" velocity="5.6548668"/>
            <dynamics damping="0.1" friction="0.0"/>
        </joint>
        <xacro:insert_transmission name="l_hip_roll_joint"/>
        <link name="l_hip_roll_link">
            <collision>
                <geometry>
                    <mesh filename="package://uthai_description/meshes/l_hip_roll.STL"/>
                </geometry>
            </collision>
