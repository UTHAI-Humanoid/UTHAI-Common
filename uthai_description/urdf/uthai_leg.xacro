                <geometry>
                    <mesh filename="package://uthai_description/meshes/l_hip_roll.STL"/>
                </geometry>
                <material name="grey"/>
            </visual>
            <inertial>
                <origin xyz="-0.00000789 -0.01513929 -0.02477769"/>
                <mass value="0.34200000"/>
                <inertia ixx="0.00032736" ixy="0.00000086" ixz="-0.00000008" iyy="0.00027354" iyz="0.00000709" izz="0.00014799" />
            </inertial>
        </link>
        <!-- left hip pitch -->
