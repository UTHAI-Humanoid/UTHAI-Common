        </joint>
        <xacro:insert_transmission name="l_ankle_pitch_joint"/>
        <link name="l_ankle_pitch_link">
            <collision>
                <geometry>
                    <mesh filename="package://uthai_description/meshes/l_ankle_pitch.STL"/>
                </geometry>
            </collision>
            <visual>
                <geometry>
                    <mesh filename="package://uthai_description/meshes/l_ankle_pitch.STL"/>
                </geometry>
                <material name="grey"/>
            </visual>
            <inertial>
                <origin xyz="-0.01513929 0.02075769 0.00000789"/>
                <mass value="0.34200000"/>
                <inertia ixx="0.00027354" ixy="-0.00000709" ixz="-0.00000086" iyy="0.00014799" iyz="-0.00000008" izz="0.00032736" />
            </inertial>
        </link>
        <!-- left ankle roll -->
        <joint name="l_ankle_roll_joint" type="revolute">
            <axis xyz="0 0 1" />
            <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
