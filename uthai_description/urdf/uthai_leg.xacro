                <geometry>
                    <mesh filename="package://uthai_description/meshes/l_knee_pitch.STL"/>
                </geometry>
                <material name="grey"/>
            </visual>
            <inertial>
                <origin xyz="0.16353031 0.00000000 0.00013618"/>
                <mass value="0.13584790"/>
                <inertia ixx="0.00005765" ixy="0.00000000" ixz="0.00000009" iyy="0.00126779" iyz="0.00000000" izz="0.00124133" />
